<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="goalkeeper.matheus.goalkeeper">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_min"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroGoleiro"
            android:label="@string/title_activity_cadastro_goleiro"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadastroPartida"
            android:label="@string/title_activity_cadastro_partida"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="goalkeeper.matheus.goalkeeper.AlteraGoleiro"
            android:label="Editar Goleiro"
            android:parentActivityName=".PerfilGoleiro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="goalkeeper.matheus.goalkeeper.Partidas"
            android:label="Lista de Partidas"
            android:parentActivityName=".PerfilGoleiro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="goalkeeper.matheus.goalkeeper.PerfilGoleiro"
            android:label="Perfil de Goleiro"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".DialogTipoJogada"
            android:label="Tipo de Jogada"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".jogadas.JogadaOfensivaTela"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jogadas.TiroMetaTela"
            android:label="Tiro de Meta"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jogadas.JogadaDefensivaTela"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VerParciais"
            android:label="Relatório Parcial de Detalhes"
            android:parentActivityName=".CadastroPartida"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jogadas.DominioTela"
            android:label="Domínio de Bola"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jogadas.ReporMaoTela"
            android:label="Reposição com as Mãos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jogadas.ReporVoleioTela"
            android:label="Reposição com Voleio"
            android:screenOrientation="portrait" />
        <activity android:name=".jogadas.DefSaidaTela"
            android:label="Defesa com Saída"
            android:screenOrientation="portrait" />
        <activity android:name=".jogadas.DefPeTela"
            android:label="Defesa com os Pés"
            android:screenOrientation="portrait"/>
        <activity android:name=".jogadas.DefCaidaTela"
            android:label="Defesa com Caída"
            android:screenOrientation="portrait" />
        <activity android:name=".jogadas.DefSobreCabecaTela"
            android:label="Defesa Sobre Cabeça"
            android:screenOrientation="portrait" />
        <activity android:name=".jogadas.DefBaseTela"
            android:label="Defesa Base"
            android:screenOrientation="portrait"/>
        <activity android:name=".jogadas.DefPunhoTela"
            android:label="Defesa com Punho"
            android:screenOrientation="portrait"/>
        <activity android:name=".Detalhe"
            android:parentActivityName=".Partidas"
            android:screenOrientation="portrait"
            android:label="Relatório Detalhes de Jogadas"></activity>
        <activity android:name=".graph.RelatorioTaticoActivity"
            android:parentActivityName=".Partidas"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".graph.PieChartActivity"
            android:parentActivityName=".Partidas"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".graph.RelatorioPorJogadaActivity"
            android:parentActivityName=".PerfilGoleiro"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".graph.RelatorioGeralActivity"
            android:parentActivityName=".PerfilGoleiro"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>